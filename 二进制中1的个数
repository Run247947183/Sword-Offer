public class Solution {
    public int NumberOf1(int n) {
        if (n == 0) {
            return 0;
        }
        String s = Integer.toBinaryString(n);
        char[] chars = s.toCharArray();
        int sum = 0;
        for (int i = 0; i < chars.length; i++) {
            if (chars[i] == '1') {
                sum++;
            }
        }
        return sum;
    }
}
