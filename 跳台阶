方法一：类似与斐波那契数列
public class Solution {
    public int JumpFloor(int target) {
        if(target <= 2){
            return target;
        }
        int pre2 = 1, pre1 = 2;
        for (int i = 3; i <= target; i++){
            int cur = pre2 + pre1;
            pre2 = pre1;
            pre1 = cur;
        }
        return pre1;
    }
}

// 方法二：递归
public class Solution {
    public int JumpFloor(int target) {
        if (target < 2) {
            return 1;
        }
        return JumpFloor(target - 1) + JumpFloor(target - 2);
    }
}
